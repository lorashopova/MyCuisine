<div class="section-title search">
    <div class="container-fluid">
        <nav class="navbar navbar-content navbar-light p-0">
            <span class="d-inline-block navbar-brand mb-0 text-uppercase">Home <span>&#187;</span> Gallery</span>
            <form class="form-inline float-md-right d-inline-block">
                <input class="form-control" type="text" id="search-input" placeholder="Search this website..." aria-label="Search">
                <button class="btn btn-outline-success my-2 my-sm-0" id="search-btn" type="button">Search</button>
            </form>
        </nav>
    </div>
</div>
</div>
<span class="gradient-line"></span>
<section>
    <div class="container-fluid">
            <div class="row gallery">
                {{#each meal}}
                <div class="col-6 col-md-4"><a href="#/meal/{{_id}}" title="meal"><img class="img-thumbnail" src="{{image._downloadURL}}" /><img class="img-thumbnail preview" src="{{image._downloadURL}}" /></a></div>
                {{/each}}
            </div>
        <nav aria-label="Page navigation">
            <ul class="pagination justify-content-end">
                {{#each this.indeces}}
                <li class="page-item"><a class="page-link" href="#/gallery/{{this}}">{{this}}</a></li>
                {{/each}}
            </ul>
        </nav>
    </div>
</section>
<span class="gradient-line"></span>