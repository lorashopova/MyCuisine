{
    // "parser": "babel-eslint",
    "env": {
        "browser": true,
        "node": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        // overrides
        "require-jsdoc": 0,
        "object-curly-spacing": [
            "error",
            "always"
        ],
        "semi": 2,
        "no-undef": 2,
        "no-use-before-define": 0,
        "no-cond-assign": 2,
        "no-console": 2,
        "no-constant-condition": 2,
        "no-control-regex": 2, 
        "no-debugger": 2, 
        "no-dupe-keys": 2, 
        "no-empty": 2, 
        "no-empty-character-class": 2, 
        "no-ex-assign": 2, 
        "no-extra-boolean-cast": 2, 
        "no-extra-parens": 0, 
        "no-extra-semi": 2,
        "no-func-assign": 2, 
        "no-inner-declarations": 2, 
        "no-invalid-regexp": 2, 
        "no-irregular-whitespace": 2, 
        "no-negated-in-lhs": 2, 
        "no-obj-calls": 2, 
        "no-regex-spaces": 2, 
        "quote-props": 0, 
        "no-sparse-arrays": 2, 
        "no-unreachable": 2, 
        "use-isnan": 2, 
        "valid-jsdoc": 2, 
        "valid-typeof": 2, 
        "block-scoped-var": 2, 
        "prefer-const": 2,
        "complexity": 2, 
        "consistent-return": 2, 
        "curly": 2, 
        "default-case": 2, 
        "dot-notation": 2, 
        "eqeqeq": 2, 
        "guard-for-in": 2, 
        "no-alert": 2, 
        "no-caller": 2, 
        "no-div-regex": 2,  
        "no-labels": 2, 
        "no-eq-null": 2, 
        "no-eval": 2, 
        "no-extend-native": 2, 
        "no-extra-bind": 2, 
        "no-fallthrough": 2, 
        "no-floating-decimal": 2, 
        "no-implied-eval": 2, 
        "no-iterator": 2,
        "no-lone-blocks": 2, 
        "no-loop-func": 2,
        "no-multi-spaces": 2, 
        "no-multi-str": 2, 
        "no-native-reassign": 2, 
        "no-new": 2, 
        "no-new-func": 2,
        "no-new-wrappers": 2, 
        "no-octal": 2,
        "no-octal-escape": 2, 
        "no-process-env": 2, 
        "no-proto": 2, 
        "no-redeclare": 2, 
        "no-return-assign": 2, 
        "no-script-url": 2, 
        "no-self-compare": 2, 
        "no-sequences": 2, 
        "no-unused-expressions": 2, 
        "no-void": 2, 
        "no-warning-comments": 2, 
        "no-with": 2, 
        "radix": 2, 
        "vars-on-top": 0, 
        "wrap-iife": 2, 
        "yoda": 0, 

        "no-catch-shadow": 2, 
        "no-delete-var": 2,
        "no-label-var": 2, 
        "no-shadow": 0, 
        "no-shadow-restricted-names": 2, 
        "no-undef-init": 2, 
        "no-undefined": 2, 
        "no-unused-vars": 1, 
        "eol-last": [
            "off"
        ],
       
        "max-len": "off",
        "max-lines": "off",
        "no-invalid-this": "off"
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true,
            "experimentalObjectRestSpread": true
        }
    },
    "globals": {
        "require": false,
        "describe": false,
        "it": false,
        "Promise": false,
        "beforeEach": false,
        "afterEach": false,
        "before": false,
        "after": false,
        "module": false,
        "Sammy": true,
        "Handlebars": true,
        "HandlebarsIntl": true,
        "System": true,
        "SystemJS": true,        
        "$": true
    }



